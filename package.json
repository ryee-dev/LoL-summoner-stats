{
  "name": "battlefy-lolstats-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "client-install": "cd client && yarn",
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd client && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "dev-build": "concurrently \"yarn server\" \"yarn heroku-postbuild\"",
    "build": "cd client && yarn && yarn build",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && yarn && yarn build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "fs": "^0.0.1-security",
    "nodemon": "^1.18.10"
  },
  "devDependencies": {}
}
